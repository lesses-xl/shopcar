.userBanner {
	width: 100%;
	height: 190rpx;
	background: rgb(171,149,108);
	box-sizing: border-box;
	padding: 10rpx;
	display: flex;
	justify-content: center;
	align-items: center;
	color: white;
}

.userinfo-img {
	width: 110rpx;
	height: 110rpx;
	/* background: skyblue; */
	border: 2rpx solid white;
	/* margin-left: -220rpx; */
	display: block;
	border-radius: 50%;
}

.userinfo-name {
	width: 320rpx;
	height: 50rpx;
	/* text-align: center; */
	font-size: 16px;
	margin-left: 20rpx;
	display: block;
	margin-top: 10rpx;
}

.my-border {
	width: 100%;
	height: 5px;
	background: rgba(238,238,238,1);
}

.my-list {
	position: relative;
	width: 100%;
	height: 90rpx;
	padding: 15px;
	font-size: 14px;
	padding-right: 22px;
	box-sizing: border-box;
	text-align: center;
	line-height: 40rpx;
	color: rgb(207, 207, 207);
}

.my-list-img {
	width: 10px;
	height: 16px;
	float: right;
}

.add {
	color: rgb(171, 150, 111);
}

.aboutwe {
	width: 260rpx;
	height: 50rpx;
	padding-left: 135rpx;
	box-sizing: border-box;
	position: relative;
}

.aboutwe image {
	position: absolute;
	width: 38rpx;
	height: 38rpx;
	float: left;
	color: white;
	left: 94rpx;
	top: 8rpx;
}

.aboutwe view {
	font-size: 26rpx;
	float: left;
	line-height: 50rpx;
}

.changemobile {
	width: 100rpx;
	height: 90rpx;
	font-size: 20rpx;
	padding: 0 5rpx;
	position: absolute;
	right: 0rpx;
	top: 0rpx;
	border: none;
	background: white;
	line-height: 90rpx;
}

.my-container .my-wrap {
	width: 32%;
	height: 185rpx;
	border: 1px solid rgb(244, 244, 244);
	display: inline-block;
	margin: 9rpx 0 0 1%;
	background: rgb(250,250,250);
	color: black;
}

.my-wrap image {
	width: 52rpx;
	height: 52rpx;
	display: inline-block;
	margin: 0 auto;
	margin-top: 8rpx;
	margin-left: 10rpx;
}

.my-wrap text {
	width: 80%;
	height: 60rpx;
	text-align: center;
	display: inline-block;
	margin: 0 auto;
	margin-left: 10rpx;
}

.my-mask {
	position: fixed;
	top: 0;
	left: 0;
	right: 0;
	bottom: 0;
	background: rgba(0, 0, 0, 0.8);
	z-index: 100;
}

.my-show {
	width: 280rpx;
	height: 200rpx;
	border: 1px solid gray;
	background: white;
	position: fixed;
	top: 50%;
	left: 50%;
	margin-left: -140rpx;
	margin-top: -180rpx;
	text-align: center;
	z-index: 999;
	font-size: 25rpx;
	border-radius: 10rpx;
}

.my-title {
	width: 100%;
	height: 70rpx;
	line-height: 70rpx;
	font-size: 32rpx;
}

.my-result {
	width: 90%;
	height: 140rpx;
	margin-top: 20rpx;
	word-wrap: break-word;
}

