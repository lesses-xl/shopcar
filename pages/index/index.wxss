}

.sm-wrap {
	width: 100%;
	height: 265rpx;
	margin-top: 17rpx;
	margin-bottom: 20rpx;
}
 
.sm-btn {
	width: 32%;
	height: 130rpx;
	/* border: 1px solid gray; */
	float: left;	
	box-sizing: border-box;
	line-height: 50rpx;
	/* margin: 10rpx 0; */
	/* padding-top: 20rpx 0 10rpx 0; */
