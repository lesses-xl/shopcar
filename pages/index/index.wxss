}

.foryou {
	width: 100%;
	height: 65rpx;
	/* border: 1px solid black; */
	text-align: center;
	font-size: 26rpx;
	line-height: 65rpx;
	background: rgb(240, 242, 245);
	padding: 10rpx 0;
}

.foryou text {
	position: relative;
	color: rgb(132,134,137);
}

.foryou text::after {
	position: absolute;
	content: '';
	width: 300rpx;
	height: 3rpx;
	background: rgb(212,213,213);
	left: 120rpx;
	top: 20rpx;
}

.foryou text::before {
	position: absolute;
	content: '';
	width: 300rpx;
	height: 3rpx;
	background: rgb(212,213,213);
	left: -314rpx;
	top: 20rpx;
}

.showyou {
	width: 100%;
	height: auto;
	background: rgb(240,242,245);
}


.bg-banner {
	width: 100%;
	height: 250rpx;
	background: rgb(241, 247, 253);
	font-size: 35rpx;
	text-align: center;
	color: #8BA0B6;
	box-sizing: border-box;
	padding: 50rpx 0 50rpx 0;
	position: relative;
	margin-bottom: 30rpx;
	margin-top: 10rpx;
}

.bg-banner-two {
	background: rgb(254,247,227);
	color: #B4A078;
}

.bg-banner-two view{
	background: #F4E9CB!important;
}

.bg-banner-two .san{
	border-right: 20rpx solid rgb(254,247,227)!important;
    border-bottom: 20rpx solid white;
    border-left: 20rpx solid rgb(254,247,227)!important;
}


.bg-banner view {
	width: 200rpx;
	height: 55rpx;
	font-size: 28rpx;
	background: rgb(216, 229, 241);
	line-height: 55rpx;
	margin: 0 auto;
	margin-top: 20rpx;
}

.bg-banner .san {
	position: absolute;
	width: 0;
    height: 0;
    border-right: 20rpx solid rgb(241, 247, 253);
    border-bottom: 20rpx solid white;
    border-left: 20rpx solid rgb(241, 247, 253);
    bottom: 0;
    left: 50%;
    margin-left: -20rpx;
}

.banner-wrap {
	height: 410rpx;
	width: 100%;
	white-space: nowrap;
	margin-bottom: 20rpx;
}

.banner-list {
	width: 36%;
	height: auto;
	/* border: 1px solid gray; */
	font-size: 28rpx;
	margin-left: 30rpx;
	display: inline-block;
	margin-bottom: 20rpx;
}

.banner-list:last-child {
	margin-right: 30rpx;
}

.banner-list-img {
	position: relative;
	display: inline-block;
	width: 100%;
	height: 280rpx;
	background: rgb(244, 244, 244);
}

.banner-list-img image {
	width: 100%;
	height: 100%;
}

.banner-list-text {
	width: 100%;
	height: auto;
}

.banner-list-text text {
	display: block;
	word-break: keep-all;
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
    box-sizing: border-box;
    padding: 0 5rpx 0 10px;
}

.banner-list-text text:nth-child(2) {
	color: #7f7f7f;
	font-size: 26rpx;
}

.banner-list-text text:nth-child(3) {
	color: red;
}

.showyou .banner-list {
	width: 45%;
	margin-left: 25rpx;
	background: rgb(244,244,244);
	border-radius: 10rpx;
}

.showyou .banner-list-img {
	height: 350rpx;
	background: rgb(230,230,230);
}

.showyou .banner-list-img image{
	border-top-left-radius: 6rpx;
	border-top-right-radius: 6rpx;
}

.showyou .banner-list-img text {
	position: absolute;
	width: 100%;
	color: #9F8A60;
	background: #F1ECE2;
	bottom: 0;
	left: 0;
	height: 65rpx;
	line-height: 65rpx;
	font-size: 24rpx;
	box-sizing: border-box;
	padding-left: 5rpx;
}

.showyou .banner-list-text text{
	padding: 10rpx 0;
	font-size: 28rpx;
	color: #333;
}

.showyou .banner-list-text text:nth-child(2){
	color: red;
	font-size: 30rpx;
	margin-top: -10rpx;
	margin-left: -5rpx;
}

.special {
	width: 100%;
